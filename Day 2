	return 0;
}

//C programs to find greatest of 3 number using ladder if-else
#include <stdio.h>
int main()
{
	int a,b,c;
	printf("Enter the value of three number:");
	scanf("%d %d %d",&a,&b,&c);
	if(a>b && a>c)
	{
		printf("a is maximum");
	}
	else if("b>a && b>c")
	{
		printf("b is maximum");
	}
	else
	{
		printf("c is maximum");
	}
	return 0;
}

//C programs to find greatest of 3 number using Nested if-else
#include <stdio.h>
int main()
{
	int a,b,c;
	printf("Enter the value of three number:");
	scanf("%d %d %d",&a,&b,&c);
	if (a>b)
	{
	if (a>c)	
	printf("a is maximum");
	else 
	printf("c is maximum");
    }
	else
	{
	if (b>c)	
	printf("b is maximum");
	else 
	printf("b is maximum");
	}
	return 0;
}
